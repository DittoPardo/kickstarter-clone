<div class="contianer">

  <div class="col-lg-5">

    <div class="row" style="margin-top: 30px">
      <a class="pull-left col-sm-3" href="#">
        <img class="img-circle" src="{{user.photo}}" style="width: 200px;height:200px;">
      </a>
      <div class="profile-section col-sm-9">
        <h2 class="media-heading">{{ user.name }}</h2>

        <div class="profile-sub-section">
          <p class="profile-details">Backed 150 Projects</p>
          <p class="profile-details"><em>{{ user.location }}</em></p>
          <a href="{{ user.website }}" class="profile-details label label-info">Website</a>
        </div>
         
        <div class="profile-bio">
          <h5>{{ user.bio }}</h5>
        </div> 
          
      </div>

    </div>

  </div>
  
</div>